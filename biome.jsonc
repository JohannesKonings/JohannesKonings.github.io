{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"files": {
		"ignore": [
			"next-blog/.next/**",
			"next-blog/out/**",
			".astro",
			"**/dist/**",
			//tanstack
			".vinxi",
			".output"
		]
	},
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": { "recommended": true }
	},
	"organizeImports": {
		"enabled": true
	}
}
