{
	"$schema": "https://biomejs.dev/schemas/2.1.3/schema.json",
	"files": {
		"includes": [
			"**/*",
			"!**/next-blog/.next/**",
			"!**/next-blog/out/**",
			"!**/.astro",
			"!**/dist/**",
			"!**/.vinxi",
			"!**/.output"
		]
	},
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": { "recommended": true }
	}
}
